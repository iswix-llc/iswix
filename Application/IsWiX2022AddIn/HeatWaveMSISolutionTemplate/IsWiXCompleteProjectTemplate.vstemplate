<VSTemplate Version="3.0.0" Type="ProjectGroup" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005">
	<TemplateData>
		<Name>IsWiX MSI Solution (WiX v4)</Name>
		<TemplateID>IsWiX.InstallerProject</TemplateID>
		<Description>A multiproject (.MSI/.MSM / .LIB) solution accelerator for creating an IsWiX / Windows Installer XML v4 based MSI.</Description>
		<Icon>IsWiX.ico</Icon>
		<ProjectType>HeatWave</ProjectType>
		<SortOrder>20</SortOrder>
		<DefaultName>ProductName</DefaultName>
		<SortOrder>4</SortOrder>
		<BuildOnLoad></BuildOnLoad>
		<CreateNewFolder>false</CreateNewFolder>
		<ProjectTypeTag>HeatWave</ProjectTypeTag>
		<LanguageTag>WiX</LanguageTag>
	</TemplateData>
	<TemplateContent></TemplateContent>
	<WizardExtension>
		<Assembly>IsWiXGlobalParams, Version=4.0.0.0, Culture=neutral, PublicKeyToken=eba4e180870619ec</Assembly>
		<FullClassName>IsWiXGlobalParams.WizardMPT</FullClassName>
	</WizardExtension>
	<WizardData>
		<Settings>
			<MappedProjectTemplate name="$projectname$Lib" path="$projectname$Lib" template="LibraryProjectTemplate\IsWiXLibraryProjectTemplate.vstemplate" />
			<MappedProjectTemplate name="$projectname$MM" path="$projectname$MM" template="MergeModuleProjectTemplate\IsWixMergeModule.vstemplate" />
			<MappedProjectTemplate name="$projectname$" path="$projectname$" template="SetupProjectTemplate\IsWixProject.vstemplate" />
		</Settings>
	</WizardData>

</VSTemplate>
